        <script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script>
        <script>
            $(document).ready(function() {
                $('#contactform-footer').submit(function(e) {
                    e.preventDefault();
                    var femail = $('.femail').val();
                    if (femail == "") {
                        $('.form-msg').fadeIn(500).html('All fields are mandatory');
                    } else {
                        $.ajax({
                            type: "POST",
                            url: 'footer-contact.php',
                            data: "femail=" + femail,
                            success: function(data) {
                                $('.form-msg').fadeIn(500).html(data);
                            },
                            error: function(data) {
                                $('.form-msg').fadeIn(500).html(data);
                            }
                        });
                        setTimeout(function() {
                            $('.form-msg').fadeOut();
                        }, 20000);
                        setTimeout(function() {
                            $('.femail').val('');
                        }, 3000);
                    }
                });
            });
        </script>
